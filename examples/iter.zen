mod stdlib;

fn iter arr func {
	let i = 0;
	let item = arr[i];

	while item != null {
		if func(item) == false {
			return null;
		}

		let i = i + 1;
		let item = arr[i];
	}
	return null;
}

fn print_item item {
	println(item);
	return true;
}

fn main {
	iter([1,2,3,4,5], print_item);
	return null;
}
