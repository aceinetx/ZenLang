mod stdlib;

fn map_mutate array func {
	let i = array_size(array) - 1;
	while i >= 0 {
		let array[i] = func(array[i]);
		let i = i - 1;
	}
	return null;
}

fn do func {
	let arr = [1,2,3,4,5];
	print(arr);
	print(" -> ");
	map_mutate(arr, func);
	println(arr);
	return null;
}

fn main {
	do(fn val {
		return val * 2;
	});
	do(fn val {
		return val / 2;
	});
	do(fn val {
		return val + val / 2;
	});
	return null;
}
